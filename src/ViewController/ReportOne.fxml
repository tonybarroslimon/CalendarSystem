<AnchorPane prefHeight="409.0" prefWidth="456.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.ReportOneController">
   <children>
      <VBox prefHeight="409.0" prefWidth="465.0">
         <children>
            <HBox prefHeight="47.0" prefWidth="388.0">
               <children>
                  <Label fx:id="reportOneLabel" text="Report One">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="20.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="300.0" prefWidth="454.0">
               <children>
                  <TableView fx:id="typeTableView" prefHeight="287.0" prefWidth="211.0">
                    <columns>
                      <TableColumn fx:id="typeColumn" prefWidth="105.0" text="Type" />
                      <TableColumn fx:id="typeNumAppointmentsColumn" prefWidth="105.0" text="# Appointments" />
                    </columns>
                     <HBox.margin>
                        <Insets left="15.0" />
                     </HBox.margin>
                  </TableView>
                  <TableView fx:id="monthTableView" prefHeight="287.0" prefWidth="211.0">
                     <columns>
                        <TableColumn fx:id="monthColumn" prefWidth="105.0" text="Month" />
                        <TableColumn fx:id="monthNumAppointmentsColumn" prefWidth="105.0" text="# Appointments" />
                     </columns>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </TableView>
               </children>
            </HBox>
            <HBox prefHeight="51.0" prefWidth="388.0">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonClicked" prefHeight="27.0" prefWidth="61.0" style="-fx-border-color: black;" text="Back">
                     <HBox.margin>
                        <Insets left="285.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
