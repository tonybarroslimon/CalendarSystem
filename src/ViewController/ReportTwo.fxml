<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="704.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.ReportTwoController">
   <children>
      <VBox prefHeight="400.0" prefWidth="705.0">
         <children>
            <HBox prefHeight="50.0" prefWidth="600.0">
               <children>
                  <Label fx:id="reportTwoLabel" text="Report Two">
                     <font>
                        <Font size="24.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="25.0" top="10.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="300.0" prefWidth="600.0">
               <children>
                  <VBox prefHeight="300.0" prefWidth="150.0">
                     <children>
                        <Button fx:id="contactOneButton" mnemonicParsing="false" onAction="#contactOneButtonPressed" prefHeight="2.0" prefWidth="110.0" style="-fx-border-color: black;" text="Contact 1">
                           <VBox.margin>
                              <Insets left="20.0" top="65.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="contactTwoButton" mnemonicParsing="false" onAction="#contactTwoButtonPressed" prefHeight="27.0" prefWidth="110.0" style="-fx-border-color: black;" text="Contact 2">
                           <VBox.margin>
                              <Insets left="20.0" top="20.0" />
                           </VBox.margin>
                        </Button>
                        <Button fx:id="contactThreeButton" mnemonicParsing="false" onAction="#contactThreeButtonPressed" prefHeight="27.0" prefWidth="110.0" style="-fx-border-color: black;" text="Contact 3">
                           <VBox.margin>
                              <Insets left="20.0" top="20.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                     <HBox.margin>
                        <Insets right="15.0" />
                     </HBox.margin>
                  </VBox>
                  <TableView fx:id="appointmentsTableView" prefHeight="300.0" prefWidth="526.0">
                    <columns>
                      <TableColumn fx:id="appointmentIDColumn" prefWidth="75.0" text="App ID" />
                      <TableColumn fx:id="titleColumn" prefWidth="75.0" text="Title" />
                        <TableColumn fx:id="typeColumn" prefWidth="75.0" text="Type" />
                        <TableColumn fx:id="descriptionColumn" prefWidth="75.0" text="Description" />
                        <TableColumn fx:id="startColumn" prefWidth="75.0" text="Start" />
                        <TableColumn fx:id="endColumn" prefWidth="75.0" text="End" />
                        <TableColumn fx:id="customerIDColumn" prefWidth="75.0" text="Customer ID" />
                    </columns>
                  </TableView>
               </children>
            </HBox>
            <HBox prefHeight="50.0" prefWidth="600.0">
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#backButtonPressed" prefHeight="29.0" prefWidth="77.0" style="-fx-border-color: black;" text="Back">
                     <HBox.margin>
                        <Insets left="575.0" top="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
